{
	"settings": {
		"analysis": {
			"tokenizer": {
				"nori_tokenizer": {
					"type": "nori_tokenizer",
					"decompound_mode": "mixed"
				},
				"standard_tokenizer": {
					"type": "standard"
				}
			},
			"filter": {
				"lowercase": {
					"type": "lowercase"
				},
				"asciifolding": {
					"type": "asciifolding"
				},
				"nori_part_of_speech": {
					"type": "nori_part_of_speech",
					"stoptags": ["E", "IC", "J", "MAG", "MAJ", "MM", "SP", "SSC", "SSO", "SC", "SE", "XPN", "XSA", "XSN", "XSV", "UNA", "NA", "VSV"]
				},
				"nori_readingform": {
					"type": "nori_readingform"
				},
				"edge_ngram_filter": {
					"type": "edge_ngram",
					"min_gram": 1,
					"max_gram": 10
				}
			},
			"analyzer": {
				"nori_analyzer": {
					"type": "custom",
					"tokenizer": "nori_tokenizer",
					"filter": [
						"lowercase",
						"asciifolding",
						"nori_readingform",
						"nori_part_of_speech",
						"edge_ngram_filter"
					]
				},
				"standard_indexing_analyzer": {
					"type": "custom",
					"tokenizer": "standard_tokenizer",
					"filter": [
						"lowercase",
						"asciifolding",
						"edge_ngram_filter"
					]
				},
				"standard_search_analyzer": {
					"type": "custom",
					"tokenizer": "standard_tokenizer",
					"filter": [
						"lowercase",
						"asciifolding"
					]
				}
			}
		}
	},
